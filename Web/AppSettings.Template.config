<?xml version="1.0"?>
<appSettings>
  <add key="ida:ClientId" value="GUID" />
  <add key="ida:Tenant" value="example.com" />
  <add key="ida:AADInstance" value="https://login.windows.net/{0}" />
  <add key="ida:Audience" value="http://localhost:63915/" />
  <add key="TokenIssuer" value="http://conference-room.com/"/>
  <add key="TokenAudience" value="http://conference-room.com/"/>
  <add key="TokenKey" value="run Convert.ToBase64String(new System.Security.Cryptography.RSACryptoServiceProvider(N).ExportCspBlob(true)) to generate a value to put here (where N is 2048 or higher)"/>
  <add key="MongoDatabaseName" value="rooms"/>
</appSettings>