@model string
@Html.DropDownListFor(_ => _, new SelectListItem[0])

<script type="text/javascript">
    $(function () {
        $.ajax({
            url: '@Url.Action("Floors", "Select")',
            method: 'post',
            success: function(data) {
                $("#@Html.IdForModel()")
                    .select2({
                        data: data
                    }).val("@Model").trigger('change');
            }
        });
    });
</script>
